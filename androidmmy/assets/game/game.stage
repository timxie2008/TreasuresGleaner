  <sound id="player_dive" src="audio/player_dive.ogg" />
  <sound id="ride_dolphin" src="audio/ride_dolphin.ogg" />
  <sound id="ride_whale" src="audio/ride_whale.ogg" />
  <sound id="pop_bubble" src="audio/bubble.ogg" />

  <layer id="game" ref="game/game.layer" />

