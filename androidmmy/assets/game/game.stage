  <model id="panel_black" src="ui/ui_panel_black.spr" />
  <model id="panel_pause" src="ui/ui_panel_pause.spr" lang="cn" />
  <model id="panel_pause" src="ui/ui_panel_pause_en.spr" lang="en" />
