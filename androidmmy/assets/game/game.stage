  <model id="label_distance" src="ui/ui_label_distance.spr" />
  <model id="label_gameover" src="ui/ui_label_gameover.spr" lang="cn"/>
  <model id="label_gameover" src="ui/ui_label_gameover_en.spr" lang="en"/>
  <model id="text_blue" src="ui/text_blue.spr" />
  <model id="label_gas" src="ui/ui_label_gas.spr" />
  <model id="label_state" src="ui/ui_label_state.spr" />
  <!-- character -->
  <model id="player" src="characters/player.spr" />
  <model id="dolphin" src="characters/dolphin.spr" />
  <model id="whale" src="characters/whale.spr" />

