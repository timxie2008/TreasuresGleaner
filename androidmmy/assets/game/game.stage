<?xml version="1.0" encoding="utf-8"?>
<stage>
  <settings>
    <key name="language" string="cn" />
  </settings>

  <!-- env -->
  <model id="sea" src="env/sea.spr" />
  <model id="light" src="env/light.spr" />
  <model id="wave" src="env/wave.spr" />
  <model id="sand" src="env/sand.spr" />
  <model id="bubble" src="env/bubble.spr" />
  <model id="coral" src="env/coral.spr" />
  <model id="grass" src="env/grass.spr" />
  <model id="cloud" src="env/cloud.spr" />
  <model id="background" src="env/background.spr" />

  <!-- items -->
  <model id="blocker-01" src="items/blocker_01.spr" />
  <model id="blocker-02" src="items/blocker_02.spr" />
  <model id="blocker-03" src="items/blocker_03.spr" />
  <model id="blocker-04" src="items/blocker_04.spr" />
  <model id="blocker-05" src="items/blocker_05.spr" />
