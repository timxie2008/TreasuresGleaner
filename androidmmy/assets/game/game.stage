  <model id="panel_quit" src="ui/ui_panel_quit.spr" lang="cn" />
  <model id="panel_quit" src="ui/ui_panel_quit_en.spr" lang="en" />
  <model id="button_yes" src="ui/ui_button_yes.spr" lang="cn" />
  <model id="button_yes" src="ui/ui_button_yes_en.spr" lang="en" />
  <model id="button_no" src="ui/ui_button_no.spr" lang="cn" />
  <model id="button_no" src="ui/ui_button_no_en.spr" lang="en" />

  <music id="music_back_1" src="audio/music_back_1.ogg" duration="14" volume="0.5" />

  <sound id="blocker_hit_1" src="audio/blocker_hit_1.ogg" />
