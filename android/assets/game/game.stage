  <model id="missle_boom" src="items/missle_boom.spr"></model>

  <!-- ui -->
  <model id="label_title" src="ui/ui_label_title.spr"></model>
  <model id="info_dist_back" src="ui/ui_info_dist_back.spr"></model>
