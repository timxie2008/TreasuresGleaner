  <model id="text_yellow" src="ui/text_yellow.spr"></model>
  <model id="button_music" src="ui/ui_button_music.spr"></model>
  <model id="button_sound" src="ui/ui_button_sound.spr"></model>
  <model id="button_play" src="ui/ui_button_play.spr"></model>
