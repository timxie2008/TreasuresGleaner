  <sound id="missle_warning" src="audio/missle_warning.ogg" />
  <sound id="pick_coin_yellow" src="audio/pick_coin_yellow.ogg" />
  <sound id="gain_coin_black" src="audio/gain_coin_black.ogg" />
  <sound id="player_die" src="audio/player_die.ogg" />
