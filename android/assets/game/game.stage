  <model id="panel_quit" src="ui/ui_panel_quit.spr"></model>
  <model id="button_yes" src="ui/ui_button_yes.spr"></model>
  <model id="button_no" src="ui/ui_button_no.spr"></model>

  <music id="music_back_1" src="audio/music_back_1.ogg" duration="6" volume="0.5"></music>

  <sound id="blocker_hit_1" src="audio/blocker_hit_1.ogg" />
