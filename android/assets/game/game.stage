<?xml version="1.0" encoding="utf-8"?>
<stage>
  <!-- env -->
  <model id="sea" src="env/sea.spr"></model>
  <model id="light" src="env/light.spr"></model>
  <model id="wave" src="env/wave.spr"></model>
  <model id="sand" src="env/sand.spr"></model>
  <model id="bubble" src="env/bubble.spr"></model>
  <model id="coral" src="env/coral.spr"></model>
